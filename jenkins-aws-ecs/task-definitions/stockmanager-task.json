{
  "family": "stockmanager",
  "containerDefinitions": [
    {
      "image": "quiram/stockmanager",
      "name": "stockmanager",
      "cpu": 10,
      "memory": 300,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8030,
          "hostPort": 8030
        }
      ],
      "healthCheck": {
        "command": [ "CMD-SHELL", "curl -f http://localhost:8030/health || exit 1" ],
        "interval": 10,
        "timeout": 1,
        "retries": 3,
        "startPeriod": 30
      }
    }
  ]
}
